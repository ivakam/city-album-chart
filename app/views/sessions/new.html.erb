<h1>Log in</h1>
<%= form_for :session, url: "/login" do |f| %>
    <div class="form-field">
        <%= f.label :email, "E-mail", required: true  %>
        <%= f.email_field :email %>
    </div>
    <div class="form-field">
        <%= f.label :password, "Password", required: true %>
        <%= f.password_field :password %>
    </div>
    <div class="form-field">
        <%= f.submit "Log in" %>
    </div>
<% end %>
